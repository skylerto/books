<div class="container">
  <div class="row">
    <h1>Analytics</h1>
  </div>
  <div class="row">
    <% @books.each do |book| %>
      <% if book.analytic %>
        <h4><%= book.title %></h4>
        <div class="views"><strong>Views</strong>: <%= book.analytic.views %></div>
        <div class="views"><strong>Sold</strong>: <%= book.analytic.sold %></div>
      <% end %>
    <% end %>
  </div>
</div>
